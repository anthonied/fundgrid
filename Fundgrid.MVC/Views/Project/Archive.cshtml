@model Fundgrid.MVC.Models.GridModel

@{
    ViewBag.Title = "Archive";
}

<h1 style="background-color:#FFCC00;">Archive</h1>
<p><font size="5">Project Name : @Html.DisplayName(@Model.ProjectName)</font></p>
<p><font size="3">Description : @Html.DisplayName(@Model.ProjectDescription)</font></p>
<p>
    
</p>
<table class="table table-condensed table-hover table-no-border">
    <tr>
        <th>Name</th>
        <th>Description</th>
        <th></th>
    </tr>

@foreach (var item in Model.Grids) 
{
    <tr>
        @*<td>
            Project
        </td>*@
        <td class="btn">
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td class="btn">
            @Html.DisplayFor(modelItem => item.Description)
        </td>

        <td class="btn">
            @Html.ActionLink("Show", "ArchiveProject", new { id=item.Id })

        </td>
    </tr>
}

</table>
@section scripts {
<script>

    var viewModel = @(Html.Raw(Json.Encode(Model)))

	    $(document).ready(function ()
	    {
	        busyViewModel.IsBusy(false);
	    });
	</script>
}